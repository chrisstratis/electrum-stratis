# Configuration file for the electrum client
# Settings defined here are shared across wallets
#
# copy this file to /etc/electrum-stratis.conf if you want read-only settings

[client]
server = electrum.stratisplatform.com:50002:t
proxy = None
gap_limit = 5
# booleans use python syntax
use_change = True
gui = qt
num_zeros = 2
# default transaction fee is in Satoshis
fee = 100000
winpos-qt = [799, 226, 877, 435]
